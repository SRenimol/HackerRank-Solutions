import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int totalcopies=a*b;
        int spends=a*c;
        
        int amount=totalcopies-spends;
        System.out.println(amount-100);
    }
}
